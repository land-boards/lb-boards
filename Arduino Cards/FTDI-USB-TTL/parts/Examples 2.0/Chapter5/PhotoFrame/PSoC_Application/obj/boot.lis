 00CE            bGetPowerSetting:
 00CE           _bGetPowerSetting:
 00CE               ; Returns value of POWER_SETTING in the A register.
 00CE               ; No inputs. No Side Effects.
 00CE               ;
                IF (POWER_SETTING & POWER_SET_2V7)
                    mov   A, POWER_SETTING | POWER_SET_SLOW_IMO
                ELSE
 00CE 5010          mov   A, POWER_SETTING          ; Supply voltage and internal main osc
                ENDIF
