                    or   reg[VLT_CR], VLT_CR_POR_MID           ;         No, change to midpoint trip
                  ELSE ; 24HMz                                 ;
 00B7 43E320        or    reg[VLT_CR], VLT_CR_POR_HIGH         ;        Yes, switch to  highest setting
                  ENDIF ; 24MHz
                 ENDIF ; Slow Mode
                ENDIF ; 5.0V Operation
                ENDIF ; 3.3V Operation
                ENDIF ; 2.7V Operation
                
 00BA 70EF          and   F, ~FLAG_XIO_MASK
 00BC           
 00BC               ;----------------------------
 00BC               ; Wrap up and invoke "main"
 00BC               ;----------------------------
 00BC           
 00BC               ; Disable the Sleep interrupt that was used for timing above.  In fact,
 00BC               ; no interrupts should be enabled now, so may as well clear the register.
 00BC               ;
 00BC 62E000        mov  reg[INT_MSK0],0
 00BF           
 00BF               ; Everything has started OK. Now select requested CPU & sleep frequency.
 00BF               ;
 00BF 7110          or    F, FLAG_XIO_MASK
 00C1 62E003        mov  reg[OSC_CR0],(SELECT_32K_JUST | PLL_MODE_JUST | SLEEP_TIMER_JUST | CPU_CLOCK_JUST)
 00C4 70EF          and   F, ~FLAG_XIO_MASK
 00C6           
 00C6               ; Global Interrupt are NOT enabled, this should be done in main().
 00C6               ; LVD is set but will not occur unless Global Interrupts are enabled.
 00C6               ; Global Interrupts should be enabled as soon as possible in main().
 00C6               ;
 00C6 62E200        mov  reg[INT_VC],0             ; Clear any pending interrupts which may
 00C9                                              ; have been set during the boot process.
                IF ENABLE_LJMP_TO_MAIN
                    ljmp  _main                    ; goto main (no return)
                ELSE
 00C9 7C0000        lcall _main                    ; call main
 00CC           .Exit:
 00CC 8FFF          jmp  .Exit                     ; Wait here after return till power-off or reset
                ENDIF
                
